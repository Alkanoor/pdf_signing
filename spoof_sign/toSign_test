% this is where the magic happens: the two signature dictionaries with
% the two document catalogs and fill bytes in between

9 0 obj
<<
	/Type /Sig
	/Filter /Adobe.PPKMS
	/SubFilter /adbe.pkcs7.detached
	/M (D:20100810191838Z)
	/Name (Gaius Julius Caesar)
	/ByteRange [0 4022 8520 5358]                  
	/Contents %%SIG%%
>>
endobj

17 0 obj
<<
	/Type /Catalog
	/Pages 4 0 R
	/AcroForm 10 0 R
>>
endobj

1 0 obj
<<
>>
endobj

